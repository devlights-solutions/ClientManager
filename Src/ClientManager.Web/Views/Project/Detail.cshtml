@model ProjectForm
@{
    ViewBag.Title = ClientManagerGlobalResources.Project;
}

<div class="modal-header">
    <button type="button" class="close" ng-click="vm.cancel()" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title">Detalle @ClientManagerGlobalResources.Project</h4>
</div>
<div class="modal-body" ng-init='vm.init(@Html.Json(Model))'>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.AngularDisplayForModel("vm.project")
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" ng-click="vm.cancel()">Cancelar</button>
</div>
